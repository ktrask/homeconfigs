URxvt.saveLines:    10000
URxvt.scrollBar:    false
#URxvt.inheritPixmap:true
URxvt.tintColor:    grey
URxvt.shading:	60
URxvt.foreground:   grey
URxvt.background:   black
urxvt*depth: 32
urxvt*background: rgba:0000/1111/0000/dddd
